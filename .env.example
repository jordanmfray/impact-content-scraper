# --- Firecrawl ---
FIRECRAWL_API_KEY="fc_live_..."

# --- OpenAI ---
OPENAI_API_KEY="sk-..."

# --- Payload ---
PAYLOAD_SECRET="long-random-string"
PAYLOAD_POSTGRES_URL="postgresql://USER:PASSWORD@HOST:5432/postgres?sslmode=require&schema=cms"
PAYLOAD_SCHEMA="cms"
PAYLOAD_SERVER_URL="http://localhost:3001"
PAYLOAD_SERVICE_TOKEN="paste-a-long-random-bearer-token"

# --- Inngest ---
INNGEST_SIGNING_KEY="ek_live_..."
INNGEST_EVENT_KEY="ek_live_..."
